	<script>
		document.addEventListener("DOMContentLoaded", function() {
    document.querySelectorAll('.btn-delete-item').forEach(button => {
        button.addEventListener('click', async function(e) {
            e.preventDefault(); // Prevent any default button behavior
            
            const productId = this.getAttribute('data-product-id');
            console.log("Product ID to delete:", productId);
            
            if (!productId) {
                Swal.fire('Error!', 'Product ID not found', 'error');
                return;
            }

            // Show confirmation dialog
			console.log(`Fetching: /deleteCartProduct/${productId}`);
            const result = await Swal.fire({
                title: 'Are you sure?',
                text: "This item will be removed from your cart",
                icon: 'warning',
                showCancelButton: true,
                confirmButtonColor: '#3085d6',
                cancelButtonColor: '#d33',
                confirmButtonText: 'Yes, remove it!'
            });
            if (result.isConfirmed) {
				try {
					const response = await fetch(`/deleteCartProduct/${productId}`, {
						method: 'DELETE',
                        headers: {
							'Content-Type': 'application/json',
                            // Add CSRF token if you're using it
                            // 'CSRF-Token': document.querySelector('meta[name="csrf-token"]').getAttribute('content')
                        },
                        // Include credentials if you're using sessions
                        credentials: 'include'
                    });

                    if (!response.ok) {
                        throw new Error(`HTTP error! status: ${response.status}`);
                    }

                    const data = await response.json();

                    if (data.success) {
                        // Remove the table row
                        const row = this.closest('.table_row');
                        row.remove();

                        // Show success message
                        Swal.fire(
                            'Deleted!',
                            'Item has been removed from your cart.',
                            'success'
                        );
						window.location.reload()
                        // Update cart total if you have this function
                        if (typeof updateCartTotal === 'function') {
                            updateCartTotal();
                        }

                        // Optionally reload the page if needed
                        // window.location.reload();
                    } else {
                        throw new Error(data.message || 'Failed to delete item');
                    }
                } catch (error) {
                    console.error('Delete error:', error);
                    Swal.fire(
                        'Error!',
                        'Failed to remove item from cart.',
                        'error'
                    );
                }
            }
        });
    });
});
	</script>
