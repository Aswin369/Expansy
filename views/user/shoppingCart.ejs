<style>
	/* Empty cart container */
.empty-cart-message {
    text-align: center;
    padding: 60px 20px;
    background: #fff;
    border-radius: 8px;
    box-shadow: 0 2px 15px rgba(0, 0, 0, 0.05);
    margin: 20px 0;
}

/* Empty cart icon */
.empty-cart-message .cart-icon {
    font-size: 60px;
    color: #e0e0e0;
    margin-bottom: 20px;
}

/* Empty cart heading */
.empty-cart-message h2 {
    font-size: 24px;
    color: #333;
    margin-bottom: 15px;
    font-weight: 600;
}

/* Empty cart description */
.empty-cart-message p {
    font-size: 16px;
    color: #666;
    margin-bottom: 30px;
    line-height: 1.5;
}

/* Shop now button */
.empty-cart-message .shop-now-btn {
    display: inline-block;
    padding: 12px 30px;
    background-color: #717fe0;
    color: #fff;
    border-radius: 25px;
    font-size: 15px;
    text-decoration: none;
    transition: all 0.3s ease;
    border: 2px solid #717fe0;
}

.empty-cart-message .shop-now-btn:hover {
    background-color: #fff;
    color: #717fe0;
    transform: translateY(-2px);
    box-shadow: 0 5px 15px rgba(113, 127, 224, 0.2);
}

/* Animation for empty cart message */
@keyframes fadeIn {
    from {
        opacity: 0;
        transform: translateY(20px);
    }
    to {
        opacity: 1;
        transform: translateY(0);
    }
}

.empty-cart-message {
    animation: fadeIn 0.5s ease-out;
}

/* Responsive adjustments */
@media (max-width: 768px) {
    .empty-cart-message {
        padding: 40px 15px;
    }
    
    .empty-cart-message h2 {
        font-size: 20px;
    }
    
    .empty-cart-message p {
        font-size: 14px;
    }
    
    .empty-cart-message .shop-now-btn {
        padding: 10px 25px;
        font-size: 14px;
    }
}
	/* Container adjustments */
.container {
    max-width: 1200px;
    margin: 0 auto;
}

/* Main cart layout */
.wrap-table-shopping-cart {
    width: 100%;
    margin-bottom: 30px;
}

/* Cart items table */
.table-shopping-cart {
    width: 100%;
    border-collapse: collapse;
}

/* Cart totals section */
.cart-totals-section {
    width: 100%;
    margin-top: 30px;
}

.bor10 {
    width: 100%;
    max-width: 100%;
    margin: 0 !important;
    padding: 40px !important;
}

/* Responsive adjustments */
@media (max-width: 768px) {
    .bor10 {
        padding: 20px !important;
    }
    
    .m-lr-0-xl {
        margin: 0 !important;
    }
}

/* Coupon and update cart section */
.flex-w.flex-sb-m.bor15 {
    width: 100%;
    margin-bottom: 30px;
}

/* Adjust column widths */
.column-1 { width: 15%; }
.column-2 { width: 25%; }
.column-3 { width: 15%; }
.column-4 { width: 20%; }
.column-5 { width: 15%; }
.column-6 { width: 10%; }
</style>
</head>
